include ../../options.mk

PROGNAME = Xenon2

WHDLOADER = $(PROGNAME).slave
WHDLOADER_CDTV = $(PROGNAME)CDTV.slave
SOURCE = $(PROGNAME)HD.s

all :  $(WHDLOADER) $(WHDLOADER_CDTV) 

$(WHDLOADER): $(SOURCE)
	$(WDATE)
	$(VASM) -o $(WHDLOADER) $(SOURCE)
$(WHDLOADER_CDTV): $(SOURCE)
	$(WDATE)
	$(VASM) -DCDTV -o $(WHDLOADER_CDTV) $(SOURCE)
