PROGNAME = OutToLunch
WHDLOADER = $(PROGNAME).slave
CD32WHDLOADER = $(PROGNAME)CD32.slave
SOURCE = $(PROGNAME)HD.asm
CD32SOURCE = $(PROGNAME)CD32HD.asm

all : $(WHDLOADER) $(CD32WHDLOADER) $(PROGNAME).islave

$(WHDLOADER): $(SOURCE)
	basm $(SOURCE)

$(CD32WHDLOADER): $(CD32SOURCE)
	basm $(CD32SOURCE)

$(PROGNAME).islave:  $(PROGNAME).islave.s
	basm  $(PROGNAME).islave.s
